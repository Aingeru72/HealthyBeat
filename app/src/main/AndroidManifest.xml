<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aingerusanchez.healthybeat">

    <!-- PERMISOS -->
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Analizar"
            android:label="@string/title_analizar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Bluetooth"
            android:label="@string/title_bluetooth">
        </activity>
        <activity
            android:name=".Archivo"
            android:label="@string/title_archivo"></activity>
        <activity
            android:name=".Resultado"
            android:label="@string/title_resultado"></activity>
        <activity
            android:name=".Perfil"
            android:label="@string/title_perfil"></activity>
    </application>

</manifest>